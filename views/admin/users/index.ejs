<%- include('../../partials/header') %>
<%- include('../../partials/navbar') %>

<div class="container">
    <div class="d-flex justify-content-between mt-5">
        <div class="d-flex align-items-center">
            <h2>Usuários cadastrados</h2>
        </div>
        <div class="d-flex align-items-center">
            <a class="btn btn-dark mb-3" href="/admin/users/create">Criar novo usuários</a>
        </div>
    </div>
    <br>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include('../../partials/footer') %>